<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   <bean id="emp" class="com.cts.model.Employee">
    <property name="SystemNo" value="32"></property>
    <property name="roomNo" value="7"></property>
    <property name="id" value="83"></property>
   <property name="name" value="keerthana"></property>
    <property name="salary" value="80000"></property>
    <property name="gender" value="f"></property>
    <property name="maritalStatus" value="single"></property>
  
     <property name="mobileNumber" value="9875676514"></property>
     <property name="account" ref="acc"></property>
   
   </bean>
   <bean id="acc" class="com.cts.model.Account">
   <property name="accountNo" value="723"/>
   <property name="accountHolder" value="keer"></property>
   </bean>
   
   </beans>